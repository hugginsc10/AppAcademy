<h1>New User</h1>
<form action="<%= users_url %>" method="post">
    <input
    type="hidden"
    name="authenticty_token"
    value="<%= form_authenticity_token %>">


    <label for="username">User Name</label>
    <input type="text" name="user[username]" id="username"  value="<%= @user.username %>">
    
    <label for="password">Password</label>
    <input type="password" name="user[passowrd]" id="password" value="<%= @user.password %>">
    
    <input type="submit">

</form>
        